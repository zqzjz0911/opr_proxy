{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/chat/completions",
      "dest": "api/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/models",
      "dest": "api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/admin/keys",
      "dest": "api/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}